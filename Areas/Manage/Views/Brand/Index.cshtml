@model List<Brand>
@{
    ViewData["Title"] = "Index";
}

<div class="row align-items-center justify-content-between">
    <h1>Brands</h1>
    <a asp-area="manage" asp-controller="brand" asp-action="create" class="btn btn-success">Create</a>
</div>
<div class="row">
    <div class="col-lg-12 tblContent">
        @await Html.PartialAsync("_BrandIndexPartial", Model) 
    </div>
</div>
